{
    "editor.formatOnSave": false,
    "eslint.enable": true,
    "eslint.debug": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": false,
        "source.fixAll.eslint": true
    },
    "eslint.options": {
        "overrideConfigFile": ".eslintrc.js"
    },
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"}